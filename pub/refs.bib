@article{thiebautCharacterizationVerticalEvolution2020,
  title = {Characterization of the Vertical Evolution of the Three-Dimensional Turbulence for Fatigue Design of Tidal Turbines},
  author = {Thi{\'e}baut, Maxime and Filipot, Jean-Fran{\c c}ois and Maisondieu, Christophe and Damblans, Guillaume and Jochum, Christian and Kilcher, Levi F. and Guillou, Sylvain},
  year = {2020},
  month = aug,
  journal = {Philosophical Transactions. Series A, Mathematical, Physical, and Engineering Sciences},
  volume = {378},
  number = {2178},
  pages = {20190495},
  issn = {1471-2962},
  doi = {10.1098/rsta.2019.0495},
  abstract = {A system of two coupled four-beam acoustic Doppler current profilers was used to collect turbulence measurements over a 36-h period at a highly energetic tidal energy site in Alderney Race. This system enables the evaluation of the six components of the Reynolds stress tensor throughout a large proportion of the water column. The present study provides mean vertical profiles of the velocity, the turbulence intensity and the integral lengthscale along the streamwise, spanwise and vertical direction of the tidal current. Based on our results and considering a tidal-stream energy convertor (TEC) aligned with the current main direction, the main elements of turbulence prone to affect the structure (material fatigue) and to alter power generation would likely be: (i) the streamwise turbulence intensity (I                         x                     ), (ii) the shear stress, [Formula: see text], (iii) the normal stress, [Formula: see text] and (iv) the vertical integral lengthscale (L                         z                     ). The streamwise turbulence intensity, (I                         x                     ), was found to be higher than that estimated at other tidal energy sites across the world for similar height above bottom. Along the vertical direction, the length (L                         z                     ) of the large-scale turbulence eddies was found to be equivalent to the rotor diameter of the TEC Sabella D10. It is considered that the turbulence metrics presented in this paper will be valuable for TECs designers, helping them optimize their designs as well as improve loading prediction through the lifetime of the machines. This article is part of the theme issue 'New insights on tidal dynamics and tidal energy harvesting in the Alderney Race'.},
  langid = {english},
  pmid = {32713316},
  keywords = {Alderney Race,coupled acoustic Doppler current profilers,Reynolds stresses,tidal-steam energy convertor,turbulence},
  file = {/Users/lkilcher/Zotero/storage/GJI7F29N/Thiebaut et al. - 2020 - Characterization of the vertical evolution of the .pdf}
}

