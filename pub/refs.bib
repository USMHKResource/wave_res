@article{guerraTurbulenceMeasurementsFiveBeam2017,
  title = {Turbulence {{Measurements}} from {{Five}}-{{Beam Acoustic Doppler Current Profilers}}},
  author = {Guerra, Maricarmen and Thomson, Jim},
  year = {2017},
  journal = {JOURNAL OF ATMOSPHERIC AND OCEANIC TECHNOLOGY},
  volume = {34},
  pages = {18},
  abstract = {Two new five-beam acoustic Doppler current profilers\textemdash the Nortek Signature1000 AD2CP and the Teledyne RDI Sentinel V50\textemdash are demonstrated to measure turbulence at two energetic tidal channels within Puget Sound, Washington. The quality of the raw data is tested by analyzing the turbulent kinetic energy frequency spectra, the turbulence spatial structure function, the shear in the profiles, and the covariance Reynolds stresses. The five-beam configuration allows for a direct estimation of the Reynolds stresses from along-beam velocity fluctuations. The Nortek's low Doppler noise and high sampling frequency allow for the observation of the turbulent inertial subrange in both the frequency spectra and the turbulence structure function. The turbulence parameters obtained from the five-beam acoustic Doppler current profilers are validated with turbulence data from simultaneous measurements with acoustic Doppler velocimeters. These combined results are then used to assess a turbulent kinetic energy budget in which depth profiles of the turbulent kinetic energy dissipation and production rates are compared. The associated codes are publicly available on the MATLAB File Exchange website.},
  langid = {english},
  file = {/Users/lkilcher/Dropbox/ZotFile/Guerra_Thomson_2017_Turbulence Measurements from Five-Beam Acoustic Doppler Current ProÔ¨Ålers.pdf}
}

