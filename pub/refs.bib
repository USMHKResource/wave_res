@article{garcia-medinaWaveResourceAssessment2014,
  title = {Wave Resource Assessment in {{Oregon}} and Southwest {{Washington}}, {{USA}}},
  author = {{Garc{\'i}a-Medina}, Gabriel and {\"O}zkan-Haller, H. Tuba and Ruggiero, Peter},
  year = {2014},
