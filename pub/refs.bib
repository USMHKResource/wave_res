@article{baneMarineHydrokineticEnergy2017,
  title = {Marine {{Hydrokinetic Energy}} from {{Western Boundary Currents}}},
  author = {Bane, John M. and He, Ruoying and Muglia, Michael and Lowcher, Caroline F. and Gong, Yanlin and Haines, Sara M.},
  year = {2017},
