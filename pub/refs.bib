@article{levineTurbulenceMeasurementAutonomous1999,
  title = {Turbulence {{Measurement}} from an {{Autonomous Underwater Vehicle}}},
  author = {Levine, Edward R. and Lueck, Rolf G.},
  year = {1999},
  journal = {Journal of Atmospheric and Oceanic Technology},
  volume = {16},
  pages = {1533--1544},
  abstract = {Horizontal profiles of the microstructure of velocity and temperature were obtained with a large autonomous underwater vehicle (AUV) using two piezoelectric shear probes, an FP07 thermistor, and three orthogonal accelerometers mounted on a sting at the forward end of the vehicle. A winter \dbend eld trial in Narragansett Bay provided a run in the midwater pycnocline at 8-m depth that contained a thermal front, an ascending pro\dbend le to 3-m depth, and a run in the weakly strati\dbend ed surface layer at this depth. Although shear spectra were strongly contaminated by narrowband vibrations produced by the motor and actuators, this contamination was highly coherent with the measured acceleration and was removed with standard signal processing techniques. The corrected spectra agreed well with the Nasmyth universal spectrum for wavenumbers up to 40 cpm. The estimated rate of dissipation of kinetic energy varied from 0.8 to 250 3 1028 W kg21 and was consistent with the rate of production of turbulence by surface wind forcing and bottom stress.},
