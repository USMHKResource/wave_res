@article{ledwellEvidenceEnhancedMixing2000,
  title = {Evidence for Enhanced Mixing over Rough Topography in the Abyssal Ocean},
  volume = {403},
  abstract = {The overturning circulation of the ocean plays an important role in modulating the Earth's climate. But whereas the mechanisms for the vertical transport of water into the deep ocean - deep water formation at high latitudes and horizontal transport in ocean currents have been largely identifed, it is not clear how the compensating vertical transport of water from the depths to the surface is accomplished. Turbulent mixing across surfaces of constant density is the only viable mechanism for reducing the density of the water and enabling it to rise. However, measurements of the internal wave field, the main source of energy for mixing, and of turbulent dissipation rates, have typically implied diffusivities across surfaces of equal density of only ,0.1 cm2 s-1, too small to account for the return \dbend{}ow. Here we report measurements of tracer dispersion and turbulent energy dissipation in the Brazil basin that reveal diffusivities of 2+/-4 cm2 s-1 at a depth of 500m above abyssal hills on the \dbend{}ank of the Mid-Atlantic Ridge, and approximately 10 cm2 s-1 nearer the bottom. This amount of mixing, probably driven by breaking internal waves that are generated by tidal currents \dbend{}owing over the rough bathymetry, may be large enough to close the buoyancy budget for the Brazil basin and suggests a mechanism for closing the global overturning circulation.},
  journal = {Nature},
  author = {Ledwell, J. R. and Montgomery, E. T. and Polzin, K. L. and Laurent, L. C. St and Schmitt, R. W. and Toole, J. M.},
  month = jan,
  year = {2000},
  keywords = {Mixing,Deep Ocean,Topography},
  pages = {179-181}
}

