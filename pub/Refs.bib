@article{wangModelingStudyTidal2017,
  series = {Wave and {{Tidal Resource Characterization}}},
  title = {A Modeling Study of Tidal Energy Extraction and the Associated Impact on Tidal Circulation in a Multi-Inlet Bay System of {{Puget Sound}}},
  volume = {114},
  issn = {0960-1481},
  abstract = {Previous tidal energy studies in Puget Sound have focused on major deep channels such as Admiralty Inlet that have a larger power potential. This paper focuses on the possibility of extracting tidal energy from minor tidal channels of Puget Sound by using a hydrodynamic model to quantify the power potential and the associated impact on tidal circulation. The study site is a multi-inlet bay system connected by two narrow inlets, Agate Pass and Rich Passage, to the Main Basin of Puget Sound. A three-dimensional hydrodynamic model was applied to the study site and validated for tidal elevations and currents. We examined three energy extraction scenarios in which turbines were deployed in each of the two passages and concurrently in both. Extracted power rates and associated changes in tidal elevation, current, tidal flux, and residence time were examined. Maximum instantaneous power rates reached 250~kW, 1550~kW, and 1800~kW, respectively, for the three energy extraction scenarios. Model results suggest that with the level of energy extraction in the three energy extraction scenarios, the impact on tidal circulation is very small. It is worth investigating the feasibility of harnessing tidal energy from minor tidal channels of Puget Sound.},
  journal = {Renewable Energy},
  doi = {10.1016/j.renene.2017.03.049},
  author = {Wang, Taiping and Yang, Zhaoqing},
  month = dec,
  year = {2017},
  keywords = {Tidal circulation,Agate Pass,In-stream tidal energy,Modeling,Puget Sound,Rich Passage},
  pages = {204-214},
  file = {/Users/lkilcher/Zotero/storage/PJDHTB4F/Wang and Yang - 2017 - A modeling study of tidal energy extraction and th.pdf;/Users/lkilcher/Zotero/storage/YJUPKARA/S0960148117302409.html}
